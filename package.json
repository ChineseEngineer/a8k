{
  "name": "imt-cli",
  "version": "0.10.4",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hxfdarling/imt.git"
  },
  "author": "zman <hxfdarling@hotmail.com>",
  "license": "MIT",
  "bin": {
    "imt": "./lib/bin/cli.js"
  },
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "dev": "tsc -w -sourceMap",
    "build": "rm -rf lib && tsc",
    "test": "jest",
    "test:w": "jest --watch",
    "lint": "eslint --cache \"{lib,bin,src}/**/*.js\"",
    "release": "npm run build && standard-version && git push --follow-tags origin"
  },
  "files": [
    "lib",
    "package-lock.json"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{json,css,scss,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{jsx,js}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/travis-cli": "^7.2.1",
    "@types/jest": "^23.3.12",
    "@types/node": "^10.12.18",
    "@vuepress/plugin-back-to-top": "^1.0.0-alpha.0",
    "@vuepress/plugin-pwa": "^1.0.0-alpha.0",
    "commitlint-config-imt": "0.0.4",
    "eslint-config-zman": "^0.2.1",
    "jest-extended": "^0.11.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2",
    "rimraf": "^2.6.3",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.10.5",
    "typescript": "^3.2.2",
    "vuepress": "^0.14.8"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "babel-preset-imt": "^0.3.0",
    "cache-loader": "^2.0.1",
    "chalk": "^2.4.2",
    "clean-css": "^4.2.1",
    "clean-webpack-plugin": "^1.0.1",
    "command-exists": "^1.2.8",
    "commander": "^2.19.0",
    "css-loader": "^2.1.0",
    "debug": "^4.1.1",
    "download-git-repo": "^1.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "events": "^3.0.0",
    "file-loader": "^3.0.1",
    "fs-extra": "^7.0.1",
    "html-inline-assets-loader": "^0.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.2",
    "ignore-loader": "^0.1.2",
    "imt-nunjucks-loader": "^1.0.0",
    "inquirer": "^6.2.1",
    "internal-ip": "^3.0.1",
    "jest": "^23.6.0",
    "jest-cli": "^23.6.0",
    "jest-config": "^23.6.0",
    "jest-environment-jsdom": "^23.4.0",
    "jest-pnp-resolver": "^1.0.2",
    "loader-utils": "^1.2.3",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.5.0",
    "moment": "^2.24.0",
    "nunjucks": "^3.1.7",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^3.0.0",
    "postcss-color-function": "^4.0.1",
    "postcss-custom-properties": "^6.3.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "pretty-ms": "^4.0.0",
    "query-parse": "^2.0.0",
    "react-error-overlay": "^5.1.2",
    "react-hot-loader": "^4.6.3",
    "resolve": "^1.10.0",
    "sass": "^1.16.1",
    "sass-loader": "^7.1.0",
    "semver": "^5.6.0",
    "shelljs": "^0.8.3",
    "sockjs-client": "^1.3.0",
    "speed-measure-webpack-plugin": "^1.3.0",
    "string.prototype.padstart": "^3.0.0",
    "strip-ansi": "^5.0.0",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "tapable": "^1.1.1",
    "terser-webpack-plugin": "^1.2.1",
    "url": "^0.11.0",
    "webpack": "4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2",
    "webpack-retry-load-plugin": "^1.1.4",
